{{ doctype() }}
{#
/**
*
* Possible vars that can be used:
* fileabsoluteUrl <- the absolute URL for the web page
* detectMobile() <- contains either "computer", "tablet" or "phone"
* controllerName() <- provides the absolute controllername (eg. Application\Controller\Index)
* getPath() <- current path (eg. / for index.html, /blog for tld.com/blog)
*/

#}




{% if 'computer' in detectMobile() %}
{% endif %}

<html lang="en">
<head>
    <meta charset="utf-8">

    {% block title %}
    {{ headTitle('Sven Anders Robbestad') }}
    {% endblock %}

    {% block meta %}
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
     {#% do headMeta('viewport', 'width=device-width, initial-scale=1.0') %#}
    {{ headMeta() }}
    {% endblock %}

    {% block link %}
    {% do headLink().appendStylesheet(basePath('/css/bootstrap.min.css')) %}
    {% do headLink().appendStylesheet(basePath('/css/bootstrap-responsive.min.css')) %}
        
    {% do headLink().appendStylesheet(basePath('/css/prettify.css')) %}
    {% do headLink().appendStylesheet(basePath('/css/code.css')) %}
    {% do headLink().appendStylesheet(basePath('/css/sh/shCore.css')) %}
    {% do headLink().appendStylesheet(basePath('/css/sh/shCoreEclipse.css')) %}
    

    {% do headLink({'rel': 'shortcut icon', 'href': '/images/favicon.ico'}) %}
    {% do headLink().appendStylesheet(basePath('/css/style.css')) %}
    
    {% do headLink({'rel': 'shortcut icon', 'href': '/images/icons/favicon.ico'}) %}
    {% do headLink({'rel': 'apple-touch-icon', 'href': '/images/icons/ios-icon.png'}) %}
    
    <!-- Will be displayed in most browsers other than old IE versions -->
    {% do headLink({'rel': 'icon', 'href': '/images/icons/vector-social-media/32px/star.png'}) %}

    {{ headLink() }}
    {% endblock %}
    

	
	
    {% block headscript %}
    <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
     {% do headScript().prependFile(basePath('/js/html5.js')) %}
    <![endif]-->
  
    {% endblock %}
</head>

<body onload="prettyPrint()">

<div class="navbar navbar-inverse navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <button type="button" class="btn btn-navbar collapsed" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="brand" href="{{ url('home') }}">Sven A Robbestad</a>
              
          <div class="nav-collapse collapse" style="height: 0px;">
            <ul class="nav">
            		
            		<li 
                    {% if 'Index' in controllerName %}
					class="active"
					{% endif %}
					><a href="{{ url('home') }}">Home</a></li>
                
                	<li 
                    {% if 'About' in controllerName %}
					class="active"
					{% endif %}
					><a href="{{ url('home') }}">About</a></li>
                
                	<li 
                    {% if 'Games' in controllerName %}
					class="active"
					{% endif %}
					><a href="{{ url('games') }}">Games</a></li>
                
                    
                    <li 
                    {% if 'blog' in getPath() %}
					class="active"
					{% endif %}
					><a href="{{ url('blog') }}">Blog</a></li>
					
                    {% if(zfcUserIdentity().id>0)  %} 
                        <li>Welcome {{zfcUserIdentity().user}}</li>
                    {% else %}
                            <li><a href="/user">Login</a></li>
                {% endif %}
                
                

					
            </ul>
          </div>
        </div>
      </div>
    </div>

<div class="container-fluid">

        <div class="span12">
           {#this: {{ zfcUserIdentity()|join }} #}
        {{ content | raw }}
		</div>
	
    
    </div>
    
     
        <div id="push">  
            <p class="muted credit">&copy; Sven A Robbestad. All rights reserved.</p>
        </div>
    </div>
    
  

<!-- REQUIRED - jQuery library -->            
<!-- load jQuery from local file -->
<script type="text/javascript" src="/js/jquery-1.8.2.min.js"></script>            
